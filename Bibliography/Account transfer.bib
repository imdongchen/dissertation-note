@article{Cai2003,
annote = {People use GIS with vague and implicit spatial concepts. To understand these concepts, shared knowledge of context within which the concepts are used are needed. The context includes:
1. the task (people use GIS most in domain task, for a certain purpose)
2. the spatial context and situation
3. the background of the user (knowledge, experiences and preferences). 


The auther solves the problem with a collaborative dialogue approach. Here the "collaborative" refers to collaboration between human and GIS. The user has solid knowledge of 1st and 3rd context while the system knows 2nd context better. By sharing the knowledge along the dialogue discourse, common concepts are built between human and GIS. 


The system will automatically decide what contextual information is missing (invoking schemas stored in its knowledge-base) and initial a subdialogue itself. "SharedPlan" theory of multi-agent collaboration is adopted to model the mental attitudes for an agent to participate in a collaborative discourse. It is said that with this model, the system is able to adapt to users with different levels of contextual knowledge.},
author = {Cai, Guoray and Wang, Hongmei and MacEachren, A},
file = {::},
journal = {Conference on Spatial Information Theory 2003, Kartause Ittingen},
pages = {287--300},
title = {{Communicating vague spatial concepts in human-GIS interactions: A collaborative dialogue approach}},
url = {http://www.springerlink.com/index/tcn8adh2kp61r3hb.pdf},
year = {2003}
}
@inproceedings{Wu2010,
address = {New York, New York, USA},
annote = {The paper discusses how mobile visual analytics can take advantage of rich physical environmental cues and computional power to help people make sense of the environment. 


Two points are highlighted: "sensemaking" and mobile context. Review on the two point points are conducted. 


Sensemaking involves top-down and bottom-up approaches, with "why" and "how" respectively. Also, instant cognitive and prior knowledge and experience are essential to sensemaking.


Visual analytics in mobile context currently only focus on information collection and communication, and location is generally the only input. The author points out that context information (loci (a locatio with people's sense what they can do in that place), people, event) should be provided, in order to help people learn the environment.


