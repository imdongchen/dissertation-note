@article{Ryfe2006a,
abstract = {Through a videotape analysis of five National Issues Forums, the author investigated the nature and role of narrative in small group deliberation. The author finds narrative is most frequent in deliberation. The author conceptualizes narrative, or storytelling, as a sequence of events organized in a way that has a beginning, middle, and an end. Storytelling is found to play an important role in overcoming two barriers of deliberation. First is the motivation to engage in deliberation. Deliberation is a disconcerting experience as it is quite different from everyday reasoning which is largely unconscious, unreflective, and unintentional. Storytelling helps participants establish an identity that places themselves in a particular relationship with the issue at hand, and this relationship may let participants feel accountable, high stake, and diverse, which are considered to be three elements that tend to motivate people to adopt a deliberative frame of mind by literature of deliberative democracy. The other barrier is a lack of knowledge. Storytelling puts the complex issue into the particulars of experience and concrete contexts, and makes the issue amenable to human understanding. Moreover, storytelling makes participants appear more sincere, trustworthy, and friendly by disclosing personal information, and thus making their claims more valid. Meanwhile, stories instill civility and friendliness into conversation in the situation of disagreement and conflict. Finally storytelling allows to build a sense of moral community without much explicit conflict or argument. In the end, the author also talks about the implication of storytelling for the way of facilitation because facilitation has an obvious impact on the nature, extent, and consequence of narrative.},
author = {Ryfe, David M.},
doi = {10.1080/00909880500420226},
file = {:Users/dong/Documents/Mendeley/Ryfe - 2006 - Narrative and Deliberation in Small Group Forums.pdf:pdf},
issn = {0090-9882},
journal = {Journal of Applied Communication Research},
keywords = {empirical,facilitation,storytelling},
mendeley-tags = {empirical,facilitation,storytelling},
month = {feb},
number = {1},
pages = {72--93},
title = {{Narrative and Deliberation in Small Group Forums}},
url = {http://www.tandfonline.com/doi/abs/10.1080/00909880500420226},
volume = {34},
year = {2006}
}
