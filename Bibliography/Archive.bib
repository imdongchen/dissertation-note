@article{McBurney2007a,
abstract = {The paper proposes a Deliberation Dialogue Framework to model the process of deliberation among computational entities (rather than among humans). The paper starts with clarification about the concept of 'deliberation' discussed in this context. The author distinguishes 'deliberation' from other forms of dialogue, such as information-seeking dialogue, inquiry dialogue, persuasion dialogue, and negotiation dialogue, in terms of the objective of the dialogue, the objective of participants, and the information the participants had available at commencement of the dialogue. Specifically the framework defines 8 processes in deliberation, 6 types of deliberative sentences, several activities (protocol) participants do in each process with various types of sentences.Finally the problem of commitment is emphasized, with the concept of commitment store to track the position and change of position upheld by participants. Assessment of the protocol is conducted in three ways: applied in human dialogue,measured agaist normative principles of deliberation, and outcome.},
annote = {Deliberation dialogues differ from other forms of dialogues in terms of the objectives of the dialogue, the objectives of participants, and the information participants had at hand at commencement of dialogues. Specifically,
1. Deliberation is different from information-seeking and inquiry dialogues because deliberation focuses on action. In constrast, the latter two aim to search for truth, in regardless of value assumptions (preference, interest, etc.)
2. Deliberation differ from negotiation in that the latter concerns the allocation of resources and avoids share of information.
3. Deliberation differ from persuasion because persuasion begins with at least one participant bearing an initially defined plan in mind and attempts to persuade other participants to agree to the plan. Instead, there is often no solution or plan at commencement of deliberation. Participants discuss, propose, reason and finally come up with a final decision.
        
        
The paper also adopts the concept of commitment store, which tracks the commitment of each participant. },
author = {McBurney, P},
doi = {10.1002/int},
file = {:Users/dong/Documents/Mendeley/McBurney - 2007 - The eightfold way of deliberation dialogue.pdf:pdf},
journal = {International Journal of Intelligent Systems},
keywords = {computational model},
mendeley-tags = {computational model},
pages = {95--132},
title = {{The eightfold way of deliberation dialogue}},
url = {http://onlinelibrary.wiley.com/doi/10.1002/int.20191/abstract},
volume = {22},
year = {2007}
}
