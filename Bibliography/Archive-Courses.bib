@article{Maciejewski2010c,
abstract = {As data sources become larger and more complex, the ability to effectively explore and analyze patterns among varying sources becomes a critical bottleneck in analytic reasoning. Incoming data contain multiple variables, high signal-to-noise ratio, and a degree of uncertainty, all of which hinder exploration, hypothesis generation/exploration, and decision making. To facilitate the exploration of such data, advanced tool sets are needed that allow the user to interact with their data in a visual environment that provides direct analytic capability for finding data aberrations or hotspots. In this paper, we present a suite of tools designed to facilitate the exploration of spatiotemporal data sets. Our system allows users to search for hotspots in both space and time, combining linked views and interactive filtering to provide users with contextual information about their data and allow the user to develop and explore their hypotheses. Statistical data models and alert detection algorithms are provided to help draw user attention to critical areas. Demographic filtering can then be further applied as hypotheses generated become fine tuned. This paper demonstrates the use of such tools on multiple geospatiotemporal data sets.},
annote = {There are two views in the system: geospatial view and time series, and the two views are linked. Early alert detection technique is used to find data aberration or hotspots.
        
Data aggregation and privacy preservation: On a base of actual location data overlays a layer represented as circles whose size stands for the size of a cluster and color stands for the percent of events.
        
Multivariate: as many as 3 variablees(3D mode)
        
Time: 
1. View historical data in "aggregate" and "increment" mode 
2. Temporal contours overlaid on spatial heatmap to explore trends.
        
Spae: To model the event distribution across the entire area, geospatial heatmap is produced using "kernel density estimation", rather than spatial histogram.
        
Attr: demographic and category filtering
        
        
        
This Visual Analytic system features:
1. Display georeferenced data events on a map and allow users to temporally scroll through data
2. Three graphs on the right show "a variety of data sources for a quick comparison of trends" (time series?)
3. Temporal control
4. Data aggregation and privacy preservation
5. Heatmap. Kernel density estimation
6. Context through color exploration
7. Time series analysis
8. Temporal contours},
author = {Maciejewski, Ross and Rudolph, Stephen and Hafen, Ryan and Abusalah, Ahmad M and Yakout, Mohamed and Ouzzani, Mourad and Cleveland, William S and Grannis, Shaun J and Ebert, David S},
doi = {10.1109/TVCG.2009.100},
file = {:Users/dong/Documents/Mendeley/Maciejewski et al. - 2010 - A visual analytics approach to understanding spatiotemporal hotspots.pdf:pdf},
issn = {1077-2626},
journal = {IEEE transactions on visualization and computer graphics},
keywords = {Algorithms,Artificial Intelligence,Computer Graphics,Computer Simulation,Computer-Assisted,Computer-Assisted: methods,Image Interpretation,Information Storage and Retrieval,Information Storage and Retrieval: methods,Models,Theoretical,User-Computer Interface,spatiotemporal visualization},
mendeley-tags = {spatiotemporal visualization},
number = {2},
pages = {205--20},
pmid = {20075482},
