@article{Osborne2009,
annote = {The author proposed locality-sensitive hashing, based on the traditional approaches. At first when I saw the word "hashing", I thought it was the Twitter hashing feature. However, it turns out that it is not. There have been problems called "new event detection" or "first story detection (FSD)" long before Twitter. The authors of this paper proporsed improved FSD and applied it to detect new event on Twitter. The problem turns out to be how to find the nearest neighborhood for a new coming event in terms of topic similarity. },
author = {Osborne, Miles and Lavrenko, Victor},
file = {::},
title = {{Streaming First Story Detection with application to Twitter}},
