@article{Grosz2006a,
abstract = {An adequate formulation of collective intentionality is crucial for understanding group activity and for modeling the mental state of participants in such activities. Although work on collective intentionality in philosophy, artificial intelligence, and cognitive science has many points of agreement, several key issues remain under debate. This paper argues that the dynamics of intention - in particular, the inter-related processes of plan-related group decision making and intention updating - play crucial roles in an explanation of collective intentionality. Furthermore, it is in these dynamic aspects that coordinated group activity differs most from individual activity. The paper specifies a model of the dynamics of agent intentions in the context of collaborative activity. Its integrated treatment of group decision making and coordinated updating of group-related intentions fills an important gap in prior accounts of collective intentionality, thus helping to resolve a long-standing debate about the nature of intentions in group activity. The paper also defines an architecture for collaboration-capable computer agents that satisfies the constraints of the model and is a natural extension of the standard architecture for resource-bounded agents operating as individuals. The new architecture is both more principled and more complete than prior architectures for collaborative multi-agent systems.},
annote = {doi: DOI: 10.1016/j.cogsys.2005.11.006},
author = {Grosz, Barbara J and Hunsberger, Luke},
file = {:Users/dong/Documents/Mendeley/Grosz, Hunsberger - 2006 - The dynamics of intention in collaborative activity.pdf:pdf},
isbn = {1389-0417},
journal = {Cognitive Systems Research},
keywords = {Agent architectures,Colaborative planning,Collective intentionality,Multi-agent systems},
number = {2-3},
pages = {259--272},
title = {{The dynamics of intention in collaborative activity}},
url = {http://www.sciencedirect.com/science/article/B6W6C-4JD0JCY-3/2/c0549c1070598cf3db631e6381346f76},
volume = {7},
year = {2006}
}
