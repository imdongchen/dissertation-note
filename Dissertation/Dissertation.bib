@article{Isenberg2011,
abstract = {This paper can be a guiding paper to start research on collaborative visual analytics, although it puts more focus on visualization. The paper starts with a discussion of the definition of collaborative visualization. Then the paper lists two categorization schemes: time-space matrix and three levels of engagement, viewing, interaction/exploring, and sharing/creating. The paper describes five scenarios of real world collaborative visualization applications. Finally, the paper points out four unique focuses of collaborative visualization: users and tasks, cognition and results, interaction and representations, and evaluation.},
annote = {Some important venues for collaborative visual analytics: Vis, InfoVis, VAST, ACM ITS, ACM CHI

social sensemaking
social data analysis

